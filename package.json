{
  "name": "neumo",
  "version": "0.0.0",
  "private": true,
  "description": "Neumorphism 2.0 UI component library for React/Next.js",
  "scripts": {
    "dev": "pnpm --filter docs dev",
    "dev:lib": "pnpm --filter neumo-ui dev",
    "dev:docs": "pnpm --filter docs dev",
    "build": "pnpm --filter neumo-ui build && pnpm --filter docs build",
    "build:lib": "pnpm --filter neumo-ui build",
    "build:docs": "pnpm --filter docs build",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "clean": "pnpm -r clean"
  },
  "keywords": [
    "neumorphism",
    "ui",
    "react",
    "components",
    "design-system"
  ],
  "author": "Anju",
  "license": "MIT",
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "typescript": "^5.7.3"
  }
}
