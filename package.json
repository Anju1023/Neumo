{
  "name": "neumo",
  "version": "0.0.0",
  "private": true,
  "description": "Neumorphism UI Component Library for React/Next.js",
  "scripts": {
    "dev": "pnpm --filter @neumo/ui dev",
    "build": "pnpm --filter @neumo/ui build",
    "docs:dev": "pnpm --filter docs dev",
    "docs:build": "pnpm --filter docs build",
    "lint": "eslint .",
    "test": "vitest",
    "clean": "rm -rf node_modules packages/*/node_modules apps/*/node_modules"
  },
  "keywords": [
    "react",
    "nextjs",
    "neumorphism",
    "ui",
    "components",
    "tailwindcss"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@9.15.0"
}
