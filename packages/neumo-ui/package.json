{
  "name": "neumo-ui",
  "version": "0.1.0",
  "description": "Neumorphism 2.0 UI component library for React/Next.js",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "neumorphism",
    "ui",
    "react",
    "components",
    "design-system",
    "tailwindcss"
  ],
  "author": "Anju",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Anju1023/Neumo.git",
    "directory": "packages/neumo-ui"
  },
  "bugs": {
    "url": "https://github.com/Anju1023/Neumo/issues"
  },
  "homepage": "https://github.com/Anju1023/Neumo#readme",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tailwindcss/cli": "^4.1.18",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitest/coverage-v8": "^4.0.17",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.18",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
